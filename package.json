{
  "name": "heart-disease-prediction-app",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:dev": "yarn build --env.env=development",
    "build:dev:watch": "yarn build:dev --watch --hot",
    "build:dev:analyze": "yarn build:dev --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "build:prod": "yarn build -p --env.env=production",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:analyze": "yarn build:prod --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "lint": "eslint ./src --ext .jsx --ext .js",
    "lint:fix": "eslint ./src --fix --ext .jsx --ext .js --fix",
    "serve:dev": "webpack-dev-server --mode development --open --hot --env.env=development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --env.addon=dashboard",
    "start": "yarn serve:dev",
    "test": "jest --runInBand --detectOpenHandles --config .jest.config.js",
    "test:watch": "jest -u --runInBand --verbose --watch --detectOpenHandles --config .jest.config.js",
    "test:coverage": "jest -u --coverage --verbose --runInBand --detectOpenHandles --config .jest.config.js",
    "docker:dev": "yarn docker:dev:build && yarn docker:dev:start",
    "docker:dev:build": "docker build -f docker/web/Dockerfile --target development -t heart-disease-prediction-app .",
    "docker:dev:start": "docker run --rm -it --network host -v $PWD:/usr/src/app heart-disease-prediction-app",
    "docker:prod": "yarn docker:prod:build && yarn docker:prod:start",
    "docker:prod:build": "docker build --build-arg API_BASE_URL=$API_BASE_URL -f docker/web/Dockerfile --target production -t heart-disease-prediction-app:production .",
    "docker:prod:start": "docker run --rm -it --network host -e API_BASE_URL=$API_BASE_URL -v $PWD:/usr/src/app heart-disease-prediction-app:production"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^2.0.0",
    "formik": "^2.1.4",
    "formik-semantic-ui": "^0.10.0",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.3.0",
    "i18next-http-backend": "^1.0.15",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dates": "^21.8.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.0.10",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "use-async-effect": "^2.2.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "inject-html-webpack-plugin": "^0.0.39",
    "jest": "^26.0.1",
    "jest-transform-stub": "^2.0.0",
    "less": "2.7.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.3",
    "tslint-loader": "^3.5.4",
    "url-loader": "1.1.2",
    "webpack": "^4.43.0",
    "webpack-async-chunk-names-plugin": "^0.1.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
